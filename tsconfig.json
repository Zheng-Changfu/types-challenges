{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext",
      "DOM"
    ],
    "module": "esnext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  },
  "include": [
    "*.ts"
  ],
  
}
